		;(function($) {
	    $(document).ready(function() {
		    	var msgCnt = 1;
		    	var markers = [];
						var latLngArray = '26.9124336,75.7872709'.split(',');
						var latLng = new google.maps.LatLng(parseFloat(latLngArray[0]),parseFloat(latLngArray[1]));
						var found = false;
						for (i = 0; i < markers.length; i++) {
							if (markers[i].position.toString() == latLng.toString()) {
								found = true;
								break;
							}
						}
						if (!found) {
							markers.push({position:latLng,name:'Jaipur, Rajasthan, India'})
						}
		    	if (markers.length > 0) {
						createMap(markers);
		    	} else {
						$('.thread-map-wrapper').hide();
		    	}

				function createMap(markers) {
					if (markers.length > 0) {
						var map = new google.maps.Map(document.getElementById('thread-map'), {
					    center: {lat: 0, lng: 0},
					    maxZoom: 15,
					    zoom: 10,
					    draggable: true
					  });
					  var bounds = new google.maps.LatLngBounds();
					  for (i = 0; i < markers.length; i++) {
							var marker = new google.maps.Marker({
				        map: map,
				        position: markers[i].position,
				        placename: markers[i].name
				      });
				      marker.addListener('click', function() {
				      	var infowindow = new google.maps.InfoWindow();
				        infowindow.setContent('<div><strong>' + this.placename + '</strong><br>');
				        infowindow.open(map, this);
				      });
				      bounds.extend(marker.position);
				      if (markers.length > 1) {
				      	map.fitBounds(bounds);
				      } else if (markers.length == 1) {
				      	map.setCenter(bounds.getCenter());
								map.setZoom(10);
				    	}
						}
		    		$('.thread-map-wrapper .thread-map-mask:visible').hide();
		    	}
				}
	    });
	  })(LITHIUM.jQuery);

